<div class="profile-container">
  <%= render partial: "users/user_card", locals: { user: @author_id } %>
    <ul class="post-comment">
      <% (1..6).each do |p| %>
        <%= render partial: "post_with_comment_card", locals: { post: p } %>
      <% end %>
    </ul>
  <button class="btn">Pagination</button>
</div>
